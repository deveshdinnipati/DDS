#include <iostream>
using namespace std;
int main() 
{
int n;
cout<<"Enter the size of array :";
cin>>n;
int *arr = new int[n];
for(int i = 0; i<n; i++)
{
    cout<<"Enter the value at arr["<<i<<"]index";
    cin>>arr[i];
}
  cout<<endl<<"entered elements are :"<<endl;
  for(int i = 0; i<n; i++)
  {
      cout<<arr[i]<<endl;
  }
}
